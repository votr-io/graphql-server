{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "download:schema": "apollo schema:download --endpoint http://localhost:5000/graphql ./src/generated/schema.json",
    "generate:client": "apollo client:codegen --endpoint http://localhost:5000/graphql --target=typescript --addTypename --outputFlat ./src/tests/generated/ --includes=src/tests/**/*.test.ts",
    "generate:serviceFactory": "apollo-client-codegen generate:serviceFactory --server=http://localhost:5000/graphql  --includes=./src/tests/**/*.gql --outputDir=./src/tests/generated",
    "test": "jest",
    "dev": "ts-node-dev --respawn src/index.ts",
    "postinstall": "npm run build && npm run migrate",
    "migrate": "db-migrate up",
    "build": "tsc",
    "start": "node build/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/graphql": "^14.0.7",
    "@types/pg": "^7.4.12",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-client-codegen": "0.0.7",
    "apollo-link": "^1.2.6",
    "apollo-server": "^2.4.0",
    "apollo-server-express": "^2.4.0",
    "cors": "^2.8.5",
    "cryptr": "^4.0.2",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.5.0",
    "express": "^4.16.4",
    "graphql": "14.0.2",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0",
    "pg": "^7.8.0",
    "pg-promise": "^8.5.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cryptr": "^4.0.0",
    "@types/jest": "^24.0.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.120",
    "@types/node-fetch": "^2.1.6",
    "@types/pg-promise": "^5.4.3",
    "@types/uuid": "^3.4.4",
    "apollo": "^2.4.4",
    "jest": "^24.1.0",
    "ts-jest": "^23.10.5",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.3"
  }
}
